<?xml version="1.0" encoding="UTF-8"?>
<project version="4">
  <component name="PhpInterpreters">
    <interpreters>
      <interpreter id="${uuid_php}" name="PHP via docker-compose" home="docker-compose://DATA" debugger_id="php.debugger.XDebug">
        <remote_data DOCKER_ACCOUNT_NAME="Docker" DOCKER_COMPOSE_SERVICE_NAME="app" DOCKER_REMOTE_PROJECT_PATH="/opt/project" INTERPRETER_PATH="php" HELPERS_PATH="/opt/.phpstorm_helpers" INITIALIZED="false" VALID="true" RUN_AS_ROOT_VIA_SUDO="false">
          <dockerComposeConfigurationPaths>
            <item value="$PROJECT_DIR$/docker-compose.yml" />
          </dockerComposeConfigurationPaths>
          <envs>
            <env name="COMPOSE_PROJECT_NAME" value="${project}_${service}" />
          </envs>
          <type_data command="EXEC" />
        </remote_data>
      </interpreter>
    </interpreters>
  </component>
  <component name="PhpUnit">
    <phpunit_settings>
      <phpunit_by_interpreter interpreter_id="${uuid_php}" configuration_file_path="/var/www/phpunit.xml" custom_loader_path="/var/www/vendor/autoload.php" phpunit_phar_path="" use_configuration_file="true" />
    </phpunit_settings>
  </component>
</project>